#include <stdio.h>

int main()
{
    int arr[5];
    printf("Enter 5 values");
    for(int i=0;i<5;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("Enter a value between 1 and 5");
    int n;
    scanf("%d",&n);
    n--;
    for(int i=n+1;i<5;i++)
    {
        arr[i-1]=arr[i];
    }
    printf("Array without element %d",n+1);
    
    for(int i=0;i<4;i++)
    {
        printf("%d ",arr[i]);
    }
}
